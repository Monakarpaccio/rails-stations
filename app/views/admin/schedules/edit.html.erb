<h1>スケジュール編集</h1>
<%= form_with(model: @schedule, url: admin_schedule_path(@schedule), method: :put) do |f| %>
  <div>
    <%= f.label :start_time, "開始時刻" %>
    <%= f.datetime_select :start_time %>
  </div>
  <div>
    <%= f.label :end_time, "終了時刻" %>
    <%= f.datetime_select :end_time %>
  </div>
  <%= f.submit '更新' %>
<% end %>
